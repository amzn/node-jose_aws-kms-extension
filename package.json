{
  "name": "node-jose-aws-kms-extension",
  "version": "0.1.0",
  "description": "This library is an extension of node-jose library with AWS KMS support",
  "author": "lynx-tech@amazon.com",
  "license": "Apache-2.0",
  "repository": "https://github.com/amzn/node-jose-aws-kms-extension.git",
  "homepage": "https://github.com/amzn/node-jose-aws-kms-extension#readme",
  "scripts": {
    "clean": "rm -rf dist && rm -rf node_modules",
    "build": "tsc",
    "watch": "tsc -w",
    "prepublishOnly": "npm run build && npm run test",
    "test": "jest --collectCoverage --collectCoverageFrom=src/**/*.{ts,js} && npm run format && npm run fix",
    "posttest": "generate-coverage-data --language typescript",
    "lint": "npx eslint --ext .ts src",
    "fix": "npx eslint --ext .ts src --fix",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\""
  },
  "jest": {
    "testMatch": [
      "**/**.test.ts"
    ],
    "transform": {
      ".(js|ts)": "ts-jest"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx|json)$",
      "package.json"
    ],
    "coverageReporters": [
      "cobertura",
      "html",
      "text"
    ]
  },
  "main": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "/dist"
  ],
  "engines": {
    "node": ">= 18.0.0"
  },
  "npm-pretty-much": {
    "runTest": "never"
  },
  "devDependencies": {
    "@types/jest": "^29.5.2",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.49.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "~5.1.3"
  },
  "dependencies": {
    "@aws-sdk/client-kms": "^3.414.0",
    "node-jose": "^2.2.0"
  }
}
